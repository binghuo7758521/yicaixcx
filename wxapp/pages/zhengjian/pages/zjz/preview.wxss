/* pages/zjz/preview.wxss */
.imagePrintContainer {
    margin: 16rpx;
    width: 718rpx;
    display: flex;
    flex-direction: column;
    background-color: #F3F7FF;
    align-items: center;
    border-radius: 10rpx;
}

.spec-name {
    text-align: center;
    font-size: 33rpx;
    color: #545454;
    margin-top: 40rpx;
}

.image-dig {
    position: relative;
    padding: 16rpx;
    width: 216rpx;
    background-color: white;
    align-self: center;
    border-radius: 8rpx;
    margin-bottom: 32rpx;
    margin-top: 32rpx;
    box-shadow: 0 2px 4px 0 rgba(172,200,255,0.60);
}

.image-print {
    width: 268rpx;
    /* height: 184rpx; */
    align-self: center;
    /* line-height: 184rpx; */
    margin-bottom: 40rpx;
    box-shadow: 0 2px 4px 0 rgba(172,200,255,0.60);
    border-radius: 2rpx;
    padding: 16rpx;
    background-color: white;
}

.zjz-icon{
  position: absolute;
  bottom: 50rpx;
  right: 30rpx;
  width: 180rpx;
  transform:rotate(-15deg);
}
.preview {
    width: 100%;
    height: 100%;
    vertical-align: middle;
}

button {
    margin-left: 62rpx;
    width: 622rpx;
    border-radius: 10rpx;
    font-size: 32rpx;
    height: 96rpx;
    line-height: 96rpx;
    background-color: #f8f8f8;
    color: #363636;
}

.blue-btn {
    margin-top: 60rpx;
    margin-bottom: 32rpx;
    background-color: #394BE5;
    color: white;
}

.circle-box {
    width: 92rpx;
    height: 92rpx;
    display: flex;
    margin: auto;
}

.circle {
    width: 76rpx;
    height: 76rpx;
    border-radius: 50%;
    margin: auto;
}

.chosenCircle {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    margin: auto;
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: center;
    background-size: 41rpx 29.6rpx;
}

.circle-white {
    width: 76rpx;
    height: 76rpx;
    border-radius: 50%;
    margin: auto;
    border: #979797 1rpx solid;
}

.chosenCircle-white {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    margin: auto;
    border: #979797 1rpx solid;
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: center;
    background-size: 41rpx 29.6rpx;
    box-shadow: 0 2px 8px 0 #CECDCD!important;
}

.color-choose {
    margin-top: 50rpx;
    display: flex;
    width: 60%;
    margin-left: 20%;
}

.alllist1{
    flex-direction:row;
    display:flex;
     
    flex-wrap:wrap;
}
.spec-list {
    width: 326rpx;
    height: 210rpx;
    margin-left: 32rpx;
    margin-top: 10rpx;
    display:flex;
    box-shadow: 0 2px 14px 0 rgba(221,221,221,0.40);
    border-radius: 10rpx;
    text-align: left;
    flex-direction:row;
  
}

.spec-title{
  font-size: 28rpx;
  width: 80rpx;
}

.img-title-box{
  width: 100rpx;
 
}
.color-box{
  width: 50%;
  text-align: center;
 

}
.color-box .blackcolor{
  
  display:flex;

}
.manum{
  width: 240rpx;
  margin: 0rpx auto;
  display: flex;
  flex-direction: row;
}
.manum text{
  width: 20rpx;
  display: block;
  background: #fff;
  height: 75rpx;
  font-size: 36rpx;
  text-align: center;
  line-height: 75rpx;
  
 
}
.manum .endnum{
  width: 80prx;
  height: 45rpx;
  text-align: center;
  background: #fff;
  border-style: dotted;
  border-width:1rpx; 
}